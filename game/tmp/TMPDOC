  //add data dans config avant le call
 {
	callbacks: {
	  preBoot: function (game) {
		game.registry.merge(myCustomObject);
	  }
	}
  }


//add data dans config after call
var game = new Phaser.Game(config);
game.config.myCustomProperty = true;
 
 
 //call une scene avec des arguments
 this.scene.start('MainScene', { scene: 'MatterScene', level: 0, socket: this.socket })

//le constructeur qui vas avec
 init(props: { scene: string; level: number; socket: Socket }) {
    const { scene, level = 0, socket } = props
    this.level = level
    this.socket = socket
    this.socket.emit('joinRoom', { scene, level })
  }

 